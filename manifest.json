{
  "name": "Test App",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Testing miscellaneous functionalities google chrome extension application",
  "icons": {
    "128": "./assets/icon/github-128.png",
    "48": "./assets/icon/github-48.png",
    "16": "./assets/icon/github-16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Count word",
    "default_icon": "/assets/icon/github-48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
